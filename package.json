{
    "name": "vue-persian-calendar-datepicker",
    "version": "1.0.0",
    "description": "Persian calendar with Iran's official holidays and Iranian calendar events and Islamic and world events. And also support for Hijri and Gregorian dates in Vue",
    "homepage": "https://github.com/mohamadhpp/vue-persian-calendar-datepicker#readme",
    "main": "./dist/module.mjs",
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/module.mjs",
            "types": "./dist/runtime/plugin.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup src/module.ts --format esm,cjs --dts",
        "prepack": "nuxt-module-build build --failOnWarn=false || exit 0",
        "dev": "nuxi dev playground",
        "dev:build": "nuxi build playground",
        "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground"
    },
    "keywords": [
        "nuxt",
        "nuxt3",
        "persian",
        "calendar",
        "jalali",
        "iran",
        "hijri",
        "vue",
        "vue3",
        "datepicker",
        "vee-validate",
        "shamsi",
        "date-picker",
        "form-validation"
    ],
    "bugs": {
        "url": "https://github.com/mohamadhpp/vue-persian-calendar-datepicker/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mohamadhpp/vue-persian-calendar-datepicker.git"
    },
    "license": "ISC",
    "author": "Mohammad Hossein Pour",
    "devDependencies": {
        "@nuxt/kit": "^4.1.3",
        "@nuxt/module-builder": "^1.0.2",
        "@nuxt/schema": "^4.1.3",
        "nuxt": "^4.1.3",
        "tsup": "^8.5.0",
        "typescript": "^5.9.3",
        "vue": "^3.5.22"
    },
    "dependencies": {
        "vue-tsc": "^3.1.1"
    }
}
